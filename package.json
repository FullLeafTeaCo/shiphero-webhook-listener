{
  "name": "shiphero-inventory-listener",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development tsx watch src/server.ts",
    "start": "node dist/src/server.js",
    "start:dev": "tsx src/server.ts",
    "register:webhooks": "tsx scripts/registerWebhooks.ts",
    "register:tote-cleared": "tsx scripts/registerToteCleared.ts",
    "register:order-packed-out": "tsx scripts/registerOrderPackedOut.ts",
    "delete:webhooks": "tsx scripts/deleteWebhooks.ts",
    "list:webhooks": "tsx scripts/listWebhooks.ts",
    "test:webhook": "tsx scripts/testWebhookEndpoint.ts",
    "test:inventory-update": "tsx scripts/testInventoryUpdate.ts",
    "test:product-search": "tsx scripts/testProductSearch.ts",
    "monitor": "tsx scripts/monitorWebhooks.ts",
    "test:send": "tsx scripts/sendTestWebhook.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "pino": "^9.7.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.0"
  }
}
